{
  "name": "waffle-map-mesh-calculator-basic",
  "version": "2.3.1",
  "description": "The mesh calculator.It conforms to the standard rigional mesh.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map-mesh-calculator-basic.git"
  },
  "scripts": {
    "build": "tsc -d",
    "fix": "eslint --fix --ext .ts .",
    "lint": "eslint --ext .ts .",
    "prepare": "yarn build",
    "test": "jest",
    "typecheck": "tsc -p . --noEmit"
  },
  "keywords": [
    "map",
    "mesh",
    "waffle-map"
  ],
  "author": "dorayakikun <amakunai.dorayaki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^24.0.4",
    "@typescript-eslint/eslint-plugin": "^1.3.0",
    "@typescript-eslint/parser": "^1.4.0",
    "eslint": "^5.9.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-prettier": "^3.0.0",
    "jest": "^23.6.0",
    "prettier": "^1.15.2",
    "typescript": "^3.1.6"
  },
  "jest": {
    "collectCoverage": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "<rootDir>/preprocessor.js"
    },
    "testMatch": [
      "**/__tests__/*.+(ts|tsx|js)"
    ],
    "testURL": "http://localhost/",
    "verbose": true
  }
}
