{
  "name": "waffle-map-mesh-calculator-basic",
  "version": "2.0.0",
  "description": "The mesh calculator.It conforms to the standard rigional mesh.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map-mesh-calculator-basic.git"
  },
  "scripts": {
    "build:flow": "flow gen-flow-files src --out-dir lib",
    "build:js": "babel src -d lib",
    "build": "npm run build:js && npm run build:flow",
    "eslint-check": "eslint --print-config .eslintrc.js | eslint-config-prettier-check",
    "fmt": "prettier --write '**/*.{js,json,md}'",
    "lint": "eslint src",
    "prebuild": "rimraf lib/*",
    "precommit": "lint-staged",
    "prepublish": "npm run precommit && npm run build",
    "test": "nyc ava"
  },
  "keywords": [
    "map",
    "mesh",
    "waffle-map"
  ],
  "author": "dorayakikun <amakunai.dorayaki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.24.1",
    "babel-eslint": "^8.2.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-flow": "^6.23.0",
    "eslint": "^4.16.0",
    "eslint-config-fbjs": "^2.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-flowtype": "^2.41.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-relay": "0.0.20",
    "flow-bin": "^0.64.0",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "nyc": "^11.0.3",
    "prettier": "^1.4.4",
    "rimraf": "^2.6.2"
  },
  "ava": {
    "babel": {
      "babelrc": true
    },
    "require": [
      "babel-register"
    ]
  },
  "lint-staged": {
    "*.{js,json,md}": [
      "prettier --write",
      "git add"
    ]
  }
}
