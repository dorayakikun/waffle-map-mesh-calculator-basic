{
  "name": "waffle-map-mesh-calculator-basic",
  "version": "2.3.3",
  "description": "The mesh calculator.It conforms to the standard rigional mesh.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "types": "lib/index.d.ts",
  "engines": {
    "node": ">=24.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map-mesh-calculator-basic.git"
  },
  "scripts": {
    "build": "tsc -d",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "lint": "biome lint .",
    "lint:fix": "biome lint --write .",
    "check": "biome check .",
    "check:fix": "biome check --write .",
    "prepare": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "typecheck": "tsc -p . --noEmit"
  },
  "keywords": [
    "map",
    "mesh",
    "waffle-map"
  ],
  "author": "dorayakikun <amakunai.dorayaki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@vitest/coverage-v8": "^2.1.9",
    "esbuild": "^0.27.2",
    "fast-check": "^3.23.2",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
}
