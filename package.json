{
  "name": "waffle-map-mesh-calculator-basic",
  "version": "1.1.2",
  "description": "The mesh calculator.It conforms to the standard rigional mesh.",
  "main": "lib/index.js",
  "files": [
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/dorayakikun/waffle-map-mesh-calculator-basic.git"
  },
  "scripts": {
    "build": "babel src -d lib",
    "fmt": "prettier --write '**/*.{js,json}'",
    "precommit": "lint-staged",
    "prepack": "npm run build && flow gen-flow-files src --out-dir lib",
    "flow": "flow",
    "test": "nyc ava"
  },
  "keywords": [
    "map",
    "mesh",
    "waffle-map"
  ],
  "author": "dorayakikun <amakunai.dorayaki@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "ava": "^0.24.0",
    "babel-cli": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "eslint": "^4.6.0",
    "eslint-config-precure": "^1.0.0",
    "flow-bin": "^0.63.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.0",
    "nyc": "^11.0.3",
    "prettier": "^1.4.4"
  },
  "ava": {
    "babel": {
      "babelrc": true
    },
    "require": [
      "babel-register"
    ]
  },
  "lint-staged": {
    ".{js,json}": [
      "fmt",
      "git add"
    ]
  }
}
