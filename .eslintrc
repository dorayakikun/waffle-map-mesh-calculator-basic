extends: 
  - fbjs
rules:
  array-bracket-spacing: 0
  arrow-spacing: 0
  brace-style: 0
  comma-dangle: 0
  comma-spacing: 0
  comma-style: 0
  computed-property-spacing: 0
  flowtype/object-type-delimiter: 0
  indent: 0
  jsx-quotes: 0
  keyword-spacing: 0
  max-len: 0
  new-parens: 0
  no-extra-parens: 0
  no-extra-semi: 0
  no-floating-decimal: 0
  no-mixed-spaces-and-tabs: 0
  no-spaced-func: 0
  quotes: 0
  react/jsx-equals-spacing: 0
  semi-spacing: 0
  semi: 0
  space-before-blocks: 0
  space-before-function-paren: 0
  space-infix-ops: 0
  space-unary-ops: 0